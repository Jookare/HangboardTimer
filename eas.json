{
  "cli": {
    "version": ">= 7.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "APP_ICON": "./assets/icon.png",
        "APP_NAME": "Hangboard Timer"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
